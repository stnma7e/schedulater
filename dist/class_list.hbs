<table id="class_list_table" class="display class_table" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th>Course Number</th>
      <th>Credit Hours</th>
      <th>Title</th>
    </tr>
  </thead>
  <tbody>
    {{#each courses}}
    <tr>
      <td>{{course_num}}</td>
      <td>{{credits}}</td>
      <td>{{title}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
